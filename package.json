{
  "name": "gemini-musical-map",
  "version": "2.0.0",
  "description": "An interactive musical map that analyzes MIDI melodies using AI, places them on a 2D map based on their characteristics, and allows users to generate variations by exploring the map.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "preprocess": "ts-node -r dotenv/config scripts/preprocess.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@tonejs/midi": "^2.0.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.7",
    "tone": "^14.7.77"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.0"
  },
  "author": "Benoit Carre",
  "license": "MIT"
}
